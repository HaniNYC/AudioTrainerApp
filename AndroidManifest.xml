<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.MeadowEast.audiotest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.MeadowEast.audiotest.MainActivity"
            android:label="@string/title_activity_main"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.MeadowEast.audiotest.MyMenu"
            android:label="@string/title_activity_my_menu" >
            <intent-filter>
                <action android:name="com.MeadowEast.audiotest.MYMENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.MeadowEast.audiotest.Email"
            android:label="@string/title_activity_email" >
             
            
            <intent-filter>
                <action android:name="com.MeadowEast.audiotest.EMAIL" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.MeadowEast.audiotest.English"
            android:label="@string/title_activity_english" >
            
            <intent-filter>
                <action android:name="com.MeadowEast.audiotest.ENGLISH" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>